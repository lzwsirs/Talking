/**index.wxss**/

.container {
  background: #f6f6f6;
  width: 100%;
    height: 100%;
}
.logo{
  width:124rpx;
  height:68rpx;
  position: absolute;
  left:0;
}
.imagesize{
  width: 90%;
  height:100%;
  margin:auto;
  display:flex;
  justify-content: center;
  position: relative;
 }
 .bg-image{
    width:100%;
    border:2px solid  black;
    background-color: rgba(0,0,0,.2);
 }
 .stytxt{
   position: absolute;
   /* max-width: 100%; */
    width:60%;
    height:20%;
    top:50%;
    animation:myfirst 2s;
	  -webkit-animation:myfirst 2s; /* Safari and Chrome */
}
@keyframes myfirst
{
	0%   {transform: scale(0.5,0.5); /* 标准语法 */}
	25%  {transform: scale(1,1)}
	50%  {transform: scale(0.5,0.5)}
	100% {transform: scale(1,1)}
}

@-webkit-keyframes myfirst /* Safari and Chrome */
{
	0%   {transform: scale(0.5,0.5); /* 标准语法 */}
	25%  {transform: scale(1.5,1.5)}
	50%  {transform: scale(0.5,0.5)}
	100% {transform: scale(1,1)}
}
.start{
  width:40%;
  height:80rpx;
  background-color:orange;
  border:2px solid black;
  position: absolute;
  border-radius:60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  animation:myse 2s ease-in-out 0s 1 alternate forwards;
	-webkit-animation:myse 2s sease-in-out 0s 1 alternate forwards; /* Safari and Chrome */
}
.startbtn{
  display:block;
  width:80%;
  height:50rpx;
}
@keyframes myse
{
	0%   {transform: scale(0.4,0.4);opacity:0.4;bottom: 2%;/* 标准语法 */}
	100% {transform: scale(1,1);opacity:1;bottom: 10%;}
}

@-webkit-keyframes myse /* Safari and Chrome */
{
	0%   {transform: scale(0.4,0.4);opacity:0.4;bottom: 2%;/* 标准语法 */}
	100% {transform: scale(1,1);opacity:1;bottom: 10%;}
}

